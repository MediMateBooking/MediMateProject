              <div class="profile-sidebar">
                <div class="widget-profile pro-widget-content">
                  <div class="profile-info-widget">
                    <a class="booking-doc-img">
                      <img
                        src="<%= key.profilePicture %>"
                        alt="User Image"
                      />
                    </a>
                    <div class="profile-det-info">
                      <h3>Dr. <%= key.name %></h3>

                      <div class="patient-details">
                        <h5 class="mb-0"><%= key.specialization.specialist %></h5>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="dashboard-widget">
                  <nav class="dashboard-menu">
                    <ul>
                      <li>
                        <a href="/doctor/<%= key.userID %>">
                          <i class="fas fa-columns"></i>
                          <span>Dashboard</span>
                        </a>
                      </li>
                      <li>
                        <a href="/doctor/docAppointments/<%= key.userID %>">
                          <i class="fas fa-calendar-check"></i>
                          <span>Appointments</span>
                        </a>
                      </li>

                      <li class="active">
                        <a href="/doctor/docReviews/<%= key.userID %>">
                          <i class="fas fa-star"></i>
                          <span>Doctor Reviews</span>
                        </a>
                      </li>
                      <li>
                        <a href="/doctor/profile/<%= key.userID %>">
                          <i class="fas fa-user-cog"></i>
                          <span>Profile Settings</span>
                        </a>
                      </li>

                      <li>
                        <a href="/doctor/password/<%= key.userID %>">
                          <i class="fas fa-lock"></i>
                          <span>Change Password</span>
                        </a>
                      </li>
                      <li>
                        <a href="/login">
                          <i class="fas fa-sign-out-alt"></i>
                          <span>Logout</span>
                        </a>
                      </li>
                    </ul>
                  </nav>
                </div>
              </div>
              <!-- /Profile Sidebar -->
						</div>




